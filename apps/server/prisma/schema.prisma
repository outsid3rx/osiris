generator client {
  provider = "prisma-client-js"
}

generator zod {
  provider = "prisma-zod-generator"
  output = "../src/shared/zod"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Settings {
  id           String @id @default(cuid())
  passwordHash String
}

model Workflow {
  id      String @id @default(cuid())
  name    String
  content Json
}
